ENTRY(_start)

MEMORY
{
	ram : ORIGIN = 0x40000000, LENGTH = 0x10000
}

SECTIONS
{
  .startup : {
    *(.startup*)
  } > ram
	.text : {
	__text_start__ = ABSOLUTE(.);
	*(.text*) *(.glue_7) *(.glue_7t);
	__text_end__ = ABSOLUTE(.);
	} >ram

	.data ALIGN(4) : {
	__data_start = ABSOLUTE(.);
	*(.data*);
	__data_end__ = ABSOLUTE(.) ;
	} >ram
}
